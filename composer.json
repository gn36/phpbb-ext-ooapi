{
	"name": "gn36/phpbb-oo-posting-api",
	"type": "phpbb-extension",
	"description": "Helper classes for creating/modifying posts, topics and PMs in phpBB to be added to vendor by other extensions.",
	"homepage": "http://phpbb.gn36.de/",
	"version": "1.0.0-dev",
	"time": "2015-01-20",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Martin Beckmann",
			"email": "gn#36@phpbb.de",
			"homepage": "http://www.phpbb.de/",
			"role": "Developer"
		},
		{
			"name": "Frank Dreyer",
			"email": "pyramide@phpbb.de",
			"homepage": "http://www.phpbb.de/",
			"role": "Former Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"extra": {
		"display-name": "OO Posting API",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.2,<3.2.*@dev"
		},
		"version-check": {
			"host": "phpbb.gn36.de",
			"directory": "/extensions/vinfo",
			"filename": "phpbb-ext-ooapi.json"
		}
	}
}
